package sleepIn;

public class SleepIn {

	public static void main(String[] args) {
		System.out.println("Sleep In? " + new SleepIn().sleepIn(false, true));
	}
	
	public boolean sleepIn(boolean weekday, boolean vacation) {
		if (weekday == false && vacation == true)
				return true;
		return false;
	}
}
